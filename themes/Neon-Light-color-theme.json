{
  "name": "Neon Light",
  "type": "light",
  "colors": {
    "editor.background": "#F7F7F7",
    "editor.foreground": "#1e1f25",
    "activityBarBadge.background": "#00B3B2",
    "sideBarTitle.foreground": "#bbbbbb",
    "activityBar.background": "#eee",
    "activityBar.foreground": "#1e1f25",
    "statusBar.background": "#eee",
    "statusBar.foreground": "#1e1f25",
    "statusBarItem.remoteBackground": "#eee",
    "statusBarItem.remoteForeground": "#1e1f25",
    "editorLineNumber.foreground": "#1e1f25",
    "editorLineNumber.activeForeground": "#1e1f25",
    "sideBarSectionHeader.background": "#EcEcEc"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#86888f"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#FF5F00"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF00FF"
      }
    },

    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00B3B2"
      }
    },

    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#7C5AAF"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#39AC75"
      }
    }
